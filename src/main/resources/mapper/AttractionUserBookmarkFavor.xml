<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.enjoytrip.dao.attraction.AttractionUserBookmarkFavorDao">
	<select id = "retrieveAttractionUserBookmarkFavor" parameterType="java.lang.Long" resultType="AttractionUserBookmarkFavor">
		select
			*
		from 
			attraction_user_bookmark_favor
		<where>
			<trim suffixOverrides="AND">
				<if test="userId!=null">
					user_id = #{userId}
				</if>
				<if test="attractionId!=null">
					attraction_id = #{attractionId}
				</if>
			</trim>
		</where>
	</select>
	
</mapper>
